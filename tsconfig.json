{
	"compilerOptions": {
		"strict": true,
		"types": ["mocha", "chai"],
		"typeRoots": ["./node_modules/@types"],
		"lib": ["ESNext"],
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"target": "ESNext",
		"esModuleInterop": true,
		"baseUrl": ".",
		"paths": {
			"@generated-types/*": ["./target/types/*"]
		}
	}
}
